<form action="{{ path('update_tag_favorites', { 'redirect' : redirect }) }}" method="post" {{ form_enctype(form) }}>
  
  {{ form_errors(form) }}

  {% if  app.environment != 'test' %}
    {% include "MuzichCoreBundle:Tag:tagsPrompt.html.twig" with { 
      'form_name'   : form_name,
      'search_tags' : favorite_tags_id
    } %}
  {% else %}
    {{ form_row(form.tags) }}
  {% endif %}

  {{ form_row(form._token) }}

  <input type="submit" />
</form>